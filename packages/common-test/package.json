  "author": "Anton Kropp",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "4.14.168"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@paradoxical-io/types": "workspace:packages/types",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "jest": ">=26.4.2"
  }
}
