{
  "name": "@paradoxical-io/common-test",
  "version": "2.3.3",
  "description": "Common test-code for jest",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!*.(i)?test.*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
